package nithish;
import java.util.Scanner;
public class sector {
	public static double sq(double a,double b){

	double mango=0;
	double mang=0;
	while(mango<a){
		mang=mang+b;
		mango++;
	}
		return mang;
		
	}
 public static double mul(double result,double nun){
	 
		double hi=0;
		double hello=0;
		while(hi<result){
			hello=hello+nun;
			hi++;
		}
			return hello;
			
	}
